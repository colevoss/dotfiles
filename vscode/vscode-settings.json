  "editor.renderWhitespace": "all",
  "workbench.startupEditor": "none",
  "editor.fontFamily": "Dank Mono",
  "editor.fontLigatures": true,
  "window.zoomLevel": 0,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
