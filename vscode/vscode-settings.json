    "text",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "vim.useSystemClipboard": true,
  "editor.tabSize": 2,
  "eslint.alwaysShowStatus": true,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true,
  "[yml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderControlCharacters": true,
  "editor.find.addExtraSpaceOnTop": false,
  "yaml.format.singleQuote": true,
  "workbench.sideBar.location": "left",
  "workbench.activityBar.visible": true,
  "vim.leader": ",",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerFontFamily": "Dank Mono",
  "vim.easymotionMarkerBackgroundColor": "#ffffff",
  "vim.easymotionMarkerForegroundColorOneChar": "#000000",
  "vim.easymotionMarkerForegroundColorTwoChar": "#000000",
  "vim.sneak": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.easymotion": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.wordWrapColumn": 100
}
